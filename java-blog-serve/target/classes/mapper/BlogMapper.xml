<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssp.vue.blog.mapper.BlogMapper">
    <insert id="saveBlog" parameterType="com.ssp.vue.blog.entity.Blog" useGeneratedKeys="true" keyProperty="id">
        insert into blog values(#{id}, #{title}, #{description}, #{content}, #{statue}, #{recommend}, #{views},
        #{createDate}, #{updateDate}, #{tid}, #{uid})
    </insert>
</mapper>
